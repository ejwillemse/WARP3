@article{ ISI:000084433000006,
Author = {Corberan, A and Marti, R and Romero, A},
Title = {{Heuristics for the mixed Rural Postman Problem}},
Journal = {{COMPUTERS \& OPERATIONS RESEARCH}},
Year = {{2000}},
Volume = {{27}},
Number = {{2}},
Pages = {{183-203}},
Month = {{FEB}},
Abstract = {{The Rural Postman Problem on a mixed graph (MRPP) consists of finding a
   minimum cost tour which traverses, at least once, the arcs and edges of
   a given subset of the arcs and edges of the graph. This problem is known
   to be NP-hard. This paper presents two heuristic approaches to solve it.
   An approximate algorithm based on the resolution of some flow and
   matching problems and a tabu search implementation is presented. The
   tabu search algorithm seeks high-quality tours by means of a switching
   mechanism in an intensification phase and two levels of diversification.
   Computational results are presented to assess the merits of the method.}},
