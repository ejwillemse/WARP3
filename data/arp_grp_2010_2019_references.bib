@inproceedings{ ISI:000356417900133,
Author = {Pelikan, Jan},
Editor = {{Talasova, J and Stoklasa, J and Talasek, T}},
Title = {{Randomized heuristics for capacitated arc routing problem}},
Booktitle = {{MATHEMATICAL METHODS IN ECONOMICS (MME 2014)}},
Year = {{2014}},
Pages = {{772-776}},
Note = {{32nd International Conference on Mathematical Methods in Economics
   (MME), Olomouc, CZECH REPUBLIC, SEP 10-12, 2014}},
Abstract = {{This paper addresses the problem of providing services or goods to the
   edges of the graph which models the roads or paths of the communication
   network. The task is to search for a closed path which contains either
   all edges or a set of desired edges, where the length of the path is
   minimized. There are many modifications of the problem: postman problem,
   rural postman problem, windy postman problem, capacitated postman
   problem. Those tasks have a number of practical applications, such as
   the optimization of municipal waste collection, route optimization
   problem for cleaning city streets, reading energy meters, delivery of
