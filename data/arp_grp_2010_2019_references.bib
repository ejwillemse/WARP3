@article{ ISI:000278504000006,
Author = {Vansteenwegen, Pieter and Souffriau, Wouter and Soerensen, Kenneth},
Title = {{Solving the mobile mapping van problem: A hybrid metaheuristic for
   capacitated arc routing with soft time windows}},
Journal = {{COMPUTERS \& OPERATIONS RESEARCH}},
Year = {{2010}},
Volume = {{37}},
Number = {{11, SI}},
Pages = {{1870-1876}},
Month = {{NOV}},
Abstract = {{Creating digital maps often requires driving around the streets in a
   so-called ``mapping van{''}. Tele Atlas, the world leading supplier of
   maps and map data, uses a fleet of such vehicles to take pictures of
   streets and road signs. Minimising the number of days that a vehicle
   needs to traverse all streets in a given region gives rise to a
   capacitated arc routing problem. A specific characteristic of this
   problem, however, is the fact that taking pictures in the direction of
   the sun should be avoided as much as possible. This requirement adds
   soft time windows to the problem.
   In this paper we solve the mobile mapping van problem by transforming it
   into a vehicle routing problem with soft time windows. We then apply a
   hybrid metaheuristic, consisting of a local search phase to decrease the
   number of days and an iterated local search phase to minimise the time
   window violations. An exact linear programming solver is embedded to
   calculate the ideal starting time of the vehicle in each street. Our
   method is tested on academic and real-life problem instances and shown
   to be effective. (C) 2009 Elsevier Ltd. All rights reserved.}},
