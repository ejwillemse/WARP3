Editor = {{Filipe, J and Cetto, JA and Ferrier, JL}},
Title = {{An efficient information exchange strategy in a distributed computing
   system - Application to the CARP}},
Booktitle = {{ICINCO 2008: PROCEEDINGS OF THE FIFTH INTERNATIONAL CONFERENCE ON
   INFORMATICS IN CONTROL, AUTOMATION AND ROBOTICS, VOL ICSO: INTELLIGENT
   CONTROL SYSTEMS AND OPTIMIZATION}},
Year = {{2008}},
