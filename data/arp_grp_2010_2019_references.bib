@article{ ISI:000349592500002,
Author = {Irnich, Stefan and Lagana, Demetrio and Schlebusch, Claudia and
   Vocaturo, Francesca},
Title = {{Two-phase branch-and-cut for the mixed capacitated general routing
   problem}},
Journal = {{EUROPEAN JOURNAL OF OPERATIONAL RESEARCH}},
