@article{ ISI:000315248100005,
Author = {Fung, Richard Y. K. and Liu, Ran and Jiang, Zhibin},
Title = {{A memetic algorithm for the open capacitated arc routing problem}},
Journal = {{TRANSPORTATION RESEARCH PART E-LOGISTICS AND TRANSPORTATION REVIEW}},
Year = {{2013}},
Volume = {{50}},
Pages = {{53-67}},
Month = {{FEB}},
Abstract = {{In this paper, an open capacitated arc routing problem (OCARP) is
   defined and considered. The OCARP seeks to find a set of minimum-cost
   open routes that can serve the tasks (i.e., required arcs) of a given
   graph, subject to the vehicle capacity and travel distance. A
   mathematical programming formulation and a lower bound are established.
   An effective memetic algorithm is developed for solving the OCARP.
   Computational experiments demonstrate that the proposed algorithm can
   produce high quality solutions within a reasonable computational time
   span, and the proposed memetic algorithm is superior to the classical
   genetic algorithm in solution quality. (C) 2012 Elsevier Ltd. All rights
   reserved.}},
DOI = {{10.1016/j.tre.2012.11.003}},
