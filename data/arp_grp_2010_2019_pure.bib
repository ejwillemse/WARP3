}

@inproceedings{ ISI:000460536600191,
Author = {Lai, Qidong and Zhang, Zizhen and Jin, Xin and Qin, Hu},
Editor = {{Reveliotis, S and Cappelleri, D and Dimarogonas, DV and Dotoli, M and Fanti, MP and Li, J and Lucena, V and Seatu, C and Xie, X and Zhu, K}},
Title = {{Forest-based Tabu Search to the Split-delivery Capacitated Arc-routing
   Problem}},
Booktitle = {{2018 IEEE 14TH INTERNATIONAL CONFERENCE ON AUTOMATION SCIENCE AND
   ENGINEERING (CASE)}},
Series = {{IEEE International Conference on Automation Science and Engineering}},
Year = {{2018}},
Pages = {{1237-1242}},
Note = {{14th IEEE International Conference on Automation Science and Engineering
   (IEEE CASE), Tech Univ Munich, Munich, GERMANY, AUG 20-24, 2018}},
Organization = {{IEEE}},
Abstract = {{Arc-routing problems (ARPs) aim to determine the least-cost vehicle
   routes of a specified arc subset in a graph. The split-delivery
   capacitated arc-routing problem (SDCARP) generalizes conventional ARPs
   by allowing an arc to be serviced by more than one vehicle. Because of
   its great complexity, the existing approaches for solving SDCARP are
   scant and most of them are meta-heuristics. In this paper, we analyze
   the characteristics of SDCARP solution structures. Then we devise a
   forest-based tabu search algorithm, which involves several forest-based
   neighborhood operators, for seeking the optimal solution. By using our
   method, the search space can be significantly reduced. The computational
   results demonstrate that our approach outperforms previous methods and
   is able to improve several state-of-the-art solutions on SDCARP
   benchmark instances.}},
ISSN = {{2161-8070}},
ISBN = {{978-1-5386-3593-3}},
Unique-ID = {{ISI:000460536600191}},
}

@article{ ISI:000456243400001,
Author = {Wohlk, Sanne and Laporte, Gilbert},
Title = {{A fast heuristic for large-scale capacitated arc routing problems}},
Journal = {{JOURNAL OF THE OPERATIONAL RESEARCH SOCIETY}},
Year = {{2018}},
Volume = {{69}},
Number = {{12}},
Pages = {{1877-1887}},
Abstract = {{The purpose of this paper is to develop a fast heuristic called FASTCARP
   for the solution of large-scale capacitated arc routing problems, with
   or without duration constraints. This study is motivated by a waste
   collection problem in Denmark. After a preprocessing phase, FASTCARP
   creates a giant tour, partitions the graph into districts, and construct
   routes within each district. It then iteratively merges and splits
   adjacent districts and reoptimises the routes. The heuristic was tested
   on 264 benchmark instances containing up to 11,640 nodes, 12,675 edges,
   8581 required edges, and 323 vehicles. FASTCARP was compared with an
   alternative heuristic called BASE and with several Path-Scanning
   algorithms. On small graphs, it was better but slower than BASE. On
   larger graphs, it was much faster and only slightly worse than BASE in
   terms of solution quality. It also outperforms the Path-Scanning
   algorithms.}},
DOI = {{10.1080/01605682.2017.1415648}},
ISSN = {{0160-5682}},
EISSN = {{1476-9360}},
ORCID-Numbers = {{Wohlk, Sanne/0000-0003-3847-7528}},
Unique-ID = {{ISI:000456243400001}},
}

@article{ ISI:000456069800010,
Author = {Ting, Ching-Jung and Tsai, Han-Shiuan},
Title = {{STREET WASHING TRUCK ROUTING WITH INTERMEDIATE REFILL FACILITIES}},
Journal = {{INTERNATIONAL JOURNAL OF INDUSTRIAL ENGINEERING-THEORY APPLICATIONS AND
   PRACTICE}},
Year = {{2018}},
Volume = {{25}},
Number = {{5, SI}},
Pages = {{674-684}},
Note = {{18th International Conference on Industrial Engineering (IUIE), Seoul,
   SOUTH KOREA, 2016}},
Abstract = {{The capacitated arc routing problem (CARP) is to determine a set of
   tours that traverse a set of required arcs with the capacitated
   limitation on vehicles at minimal cost. There are many practical
   applications for variants of the CARP, including street washing and snow
   removal. The aim of this paper is to discuss the street washing truck
   routing problem with intermediate refill facilities (SWRPIR), a variant
   of the capacitated arc routing problem. Routes for street washing trucks
   are normally planned in advance and must be designed to take into
   account the vehicle capacity. Street washing vehicles could refill water
   at intermediate refill points without going back to the depot to save
   time. Due to that SWRPIR is an NP-hard problem, we proposed an ant
   colony optimization (ACO) algorithm to solve the problem. The ACO is
   tested with benchmark instances and real-world cases. The results
   indicate that our ACO is competitive in benchmark instances with
   compared algorithms and can reduce the total distance for the washing
   vehicles for real-world cases.}},
ISSN = {{1943-670X}},
Unique-ID = {{ISI:000456069800010}},
}

@inproceedings{ ISI:000446104500082,
Author = {Steyn, Llewellyn J. and Willemse, Elias J.},
Book-Group-Author = {{IEEE}},
Title = {{Using Vehicle GPS Data to Infer Offloading Times of Waste Collection
   Vehicles at Transfer Stations}},
Booktitle = {{2018 INTERNATIONAL CONFERENCE ON ADVANCES IN BIG DATA, COMPUTING AND
   DATA COMMUNICATION SYSTEMS (ICABCD)}},
Year = {{2018}},
Note = {{International Conference on Advances in Big Data, Computing and Data
   Communication Systems (icABCD), Durban, SOUTH AFRICA, AUG 06-07, 2018}},
Organization = {{IEEE region 8; IEEE Mauritius Subsect; Soc Informat Technologists \&
   Entrepreneurs; Durban Univ Technol; Natl Elect Media Inst S Africa;
   Pearson; IEEE S Africa Sect Comp Soc Chapter}},
Abstract = {{Residential waste collection is an expensive but essential service.
   Since such a large portion of the cost associated with residential waste
   collection is attributable to transportation it has become the focus of
   numerous studies on vehicle routing and optimisation. While vehicle
   routing models have great potential for reducing waste collection costs,
   a key challenge remains the input data used in these models. Literature
   focuses predominantly on problem formulation and solution methods as
   opposed to developing methods to estimate input parameters. Big data,
   and in particular GPS devices fitted on waste collection vehicles, offer
   a great opportunity to address this shortcoming. This is demonstrated,
   in this paper, by analysing GPS records of waste collection vehicles in
   and around a waste transfer station. Through the analysis we show that
   the duration of waste dropoffs follow a power-law distribution, whereas
   vehicle routing models treat it as deterministic. The fitted
   distribution can be used in future studies to evaluate the operational
   impact of stochastic drop-off times on routes developed using
   traditional routing models. We further analyse whether congestion in the
   transfer station results in an increase in drop-off duration and found
   that it does not have a major impact.}},
ISBN = {{978-1-5386-3060-0}},
Unique-ID = {{ISI:000446104500082}},
}

@article{ ISI:000439162300064,
Author = {Su, Zhou and De Schutter, Bart},
Title = {{Optimal scheduling of track maintenance activities for railway networks}},
Journal = {{IFAC PAPERSONLINE}},
Year = {{2018}},
Volume = {{51}},
Number = {{9}},
Pages = {{386-391}},
Note = {{15th International-Federation-of-Automatic-Control (IFAC) Symposium on
   Control in Transportation Systems (CTS), ITALY, JUN 06-08, 2018}},
Organization = {{Int Federat Automat Control; Int Federat Automat Control, Tech Comm 7 4
   Transportat Syst; Int Federat Automat Control, Tech Comm 7 1 Automot
   Control; Int Federat Automat Control, Tech Comm 7 2 Marine Syst; Int
   Federat Automat Control, Tech Comm 7 3 Aerosp; Int Federat Automat
   Control, Tech Comm 7 5 Intelligent Autonomous Vehicles; Univ Genova,
   Dept Informat Bioengineering Robot \& Syst Engn}},
Abstract = {{We consider optimal scheduling of track maintenance activities for a
   railway network divided into sections. The goal is to find an optimal
   time schedule for the maintenance activities and optimal routes for the
   maintenance crew (including all necessary equipment and technicians)
   that minimize the total setup costs and the travel costs over the whole
   planning horizon. The maintenance time budget, which can be the same,
   different, or flexible for each period, and the minimum time to maintain
   a section are also taken into account. We recast the track maintenance
   scheduling problem with three different settings as three variants of
   the Capacitated Arc Routing Problem with Fixed cost (CARPF), which are
   solved by transforming them into three node routing problems. The
   proposed approach is demonstrated using a case study of a part of the
   Dutch regional network. (C) 2018, IFAC (International Federation of
   Automatic Control) Hosting by Elsevier Ltd. All rights reserved.}},
DOI = {{10.1016/j.ifacol.2018.07.063}},
ISSN = {{2405-8963}},
