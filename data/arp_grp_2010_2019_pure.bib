Unique-ID = {{ISI:000384228700009}},
}

@article{ ISI:000384854700001,
Author = {Willemse, Elias J. and Joubert, Johan W.},
Title = {{Splitting procedures for the Mixed Capacitated Arc Routing Problem under
   Time restrictions with Intermediate Facilities}},
Journal = {{OPERATIONS RESEARCH LETTERS}},
Year = {{2016}},
Volume = {{44}},
Number = {{5}},
Pages = {{569-574}},
Month = {{SEP}},
Abstract = {{This paper develops optimal and quick near-optimal splitting procedures
   for the Mixed Capacitated Arc Routing Problem under Time restrictions
   with Intermediate Facilities. Splitting procedures are a key component
   of giant tour-based solution methods for Arc Routing Problems. The
   optimal and near-optimal splitting procedures are tested within a
   multi-start constructive heuristic, and a fixed execution-time limit is
   imposed. Results on benchmark instances show that the
   constructive-heuristic linked with the new optimal splitting algorithm
   performs better than the near-optimal versions. (C) 2016 Elsevier B.V.
   All rights reserved.}},
DOI = {{10.1016/j.orl.2016.06.001}},
ISSN = {{0167-6377}},
